{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "items": {
      "type": "json",
      "required": true
    },
    "total": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "orderStatus": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "processing",
        "completed",
        "cancelled"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    }
  }
}
